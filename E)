float Vector[4];
float F;
F = A+B+C+D+E/5;
if(V[0]>F){cout<<"A>"<<endl;}else cout<<"<A"<<endl;
if(V[1]>F){cout<<"B>"<<endl;}else cout<<"<B"<<endl;
